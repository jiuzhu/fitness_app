<h1>User #<%= @user.id %></h1>

<dl>
  <dt>First name</dt>
  <dd><%= @user.first_name %></dd>

  <dt>Last name</dt>
  <dd><%= @user.last_name %></dd>

  <dt>Email</dt>
  <dd><%= @user.email %></dd>

  <dt>Password</dt>
  <dd><%= @user.password %></dd>

  <dt>Age</dt>
  <dd><%= @user.age %></dd>

  <dt>Address</dt>
  <dd><%= @user.address %></dd>

  <dt>Zip</dt>
  <dd><%= @user.zip %></dd>

</dl>
